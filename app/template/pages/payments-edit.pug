extends ../main.layout.pug

block title
  |  Создание нового способа оплаты 


block content
  include ../inc/head
  include ../inc/sidebar-left.pug
  section#page-content.page__addcat.page
    .header-content
      h2 Создание нового способа оплаты 
      .breadcrumb-wrapper.hidden-xs
        ol.breadcrumb
          li
            i.fa.fa-home
            a(href='/') Главная
            i.fa.fa-angle-right
          li.active Создание нового способа оплаты  


    .body-content
      .row
        .col-md-12
          .panel.panel-success.shadow
            .panel-heading
              .pull-left
                h3.panel-title Параметры страницы
              .pull-right
                button.btn.btn-sm(data-action='collapse', data-container='body', data-toggle='tooltip', data-placement='top', data-title='Collapse', data-original-title='', title='')
                  i.fa.fa-angle-up 
              .clearfix

            .panel-body.padding-15
              .row 
                .col-md-6
                  .form-group
                    label.control-label Название 
                    input.form-control(type='text', placeholder='Введите название')
                
            
                .col-md-3
                  .form-group
                    label.control-label Активная
                    .switchs
                      input.switch(type='checkbox',  name='switch', checked='', data-on-text='ON', data-off-text='OFF', data-on-color='teal') 
                .col-md-3
                  .form-group
                    label.control-label Предлагать квитанцию
                    .switchs
                      input.switch(type='checkbox',  name='switch', data-on-text='ON', data-off-text='OFF', data-on-color='teal') 

            
          
          .panel.panel-success.shadow
            .panel-heading
              .pull-left
                h3.panel-title Описание
              .pull-right
                button.btn.btn-sm(data-action='collapse', data-container='body', data-toggle='tooltip', data-placement='top', data-title='Collapse', data-original-title='', title='')
                  i.fa.fa-angle-up             
              .clearfix
            
     
            .panel-body
              .inner-all
                form.form-horizontal
                  .form-group
                    textarea#summernote-textarea.form-control(rows='10', placeholder='Enter text ...', style='display: none;')
                    .note-editor
                      .note-dropzone
                        .note-dropzone-message
                      .note-dialog
                        .note-image-dialog.modal(aria-hidden='false')
                          .modal-dialog
                            .modal-content
                              .modal-header
                                button.close(type='button', aria-hidden='true', tabindex='-1') ×
                                h4.modal-title Insert Image
                              .modal-body
                                .row-fluid
                                  .note-group-select-from-files
                                    h5 Select from files
                                    input.note-image-input(type='file', name='files', accept='image/*')
                                  h5 Image URL
                                  input.note-image-url.form-control.span12(type='text')
                              .modal-footer
                                button.btn.btn-primary.note-image-btn.disabled(href='#', disabled='') Insert Image
                        .note-link-dialog.modal(aria-hidden='false')
                          .modal-dialog
                            .modal-content
                              .modal-header
                                button.close(type='button', aria-hidden='true', tabindex='-1') ×
                                h4.modal-title Insert Link
                              form.note-modal-form
                                .modal-body
                                  .row-fluid
                                    .form-group
                                      label Text to display
                                      input.note-link-text.form-control.span12(type='text')
                                    .form-group
                                      label To what URL should this link go?
                                      input.note-link-url.form-control.span12(type='text')
                                    .checkbox
                                      label
                                        input(type='checkbox', checked='')
                                        |  Open in new window
                                .modal-footer
                                  button.btn.btn-primary.note-link-btn.disabled(href='#', disabled='') Insert Link
                        .note-video-dialog.modal(aria-hidden='false')
                          .modal-dialog
                            .modal-content
                              .modal-header
                                button.close(type='button', aria-hidden='true', tabindex='-1') ×
                                h4.modal-title Insert Video
                              form.note-modal-form
                                .modal-body
                                  .row-fluid
                                    .form-group
                                      label Video URL?
                                      small.text-muted (YouTube, Vimeo, Vine, Instagram, DailyMotion or Youku)
                                      input.note-video-url.form-control.span12(type='text')
                                .modal-footer
                                  button.btn.btn-primary.note-video-btn.disabled(href='#', disabled='') Insert Video
